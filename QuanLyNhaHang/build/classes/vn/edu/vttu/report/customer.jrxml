	<subDataset name="New Dataset 1" uuid="38bcf447-2cc3-478d-b6eb-b84b834b1f36">
		<queryString>
			<![CDATA[SELECT
	customer.id ,
	customer.name,
	CASE customer.sex WHEN TRUE THEN 'Nam' ELSE 'Nữ' END AS gioitinh,
	DATE_FORMAT(customer.birthDay,'%d/%m/%Y') AS ngaysinh,	
	ROUND(DATEDIFF(NOW(),customer.birthDay)/360) AS tuoi,
	customer.phone AS dienthoai,
	customer.address AS diachi,
	customer.email AS email
FROM customer WHERE customer.isActive=TRUE;]]>
		</queryString>
